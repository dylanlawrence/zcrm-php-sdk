{
    "name": "dylanlawrence/zohocrm",
    "description": "Zoho CRM API SDK for PHP",
    "type": "library",
    "homepage": "https://github.com/zoho/zcrm-php-sdk",
    "require": {
        "symfony/yaml": "4.*",
        "ext-curl": "^7.4",
        "php": "^7.4"
    },
    "require-dev": {
        "phpunit/phpunit": "10.0.x-dev"
    },
    "authors": [
        {
            "name": "Dylan Lawrence",
            "email": "dylan@visualidentitygroup.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "ZCRM\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ZCRM\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable"
}
